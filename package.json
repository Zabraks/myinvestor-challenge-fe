    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,md}": [
      "eslint --max-warnings=0",
      "prettier --check"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
